'      }',
'',
'      // Specifically: any column whose header text contains "Fantasy Points" -> 1 dp.',
'      function roundFantasyPointsIn($t){',
'        if(!$t || !$t.length) return;',
'        const fpIdx = [];',
'        $t.find("thead th").each(function(i){',
