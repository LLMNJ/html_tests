'        if(!fpIdx.length) return;',
'        const roundRow = function($row){',
'          const $cells = $row.children();',
'          fpIdx.forEach(function(i){',
'            const $c = $cells.eq(i);',
'            const n = parseFloat(($c.text()||"").trim());',
'            if(!isNaN(n)){',
